{
  "name": "recmap-frontend",
  "version": "0.1.0",
  "description": "recmap-frontend",
  "main": "./src/index.tsx",
  "scripts": {
    "watch": "watchify -t node-lessify ./src/index.tsx --debug -p [ tsify --noImplicitAny ]  -o ./build/app.js",
    "build": "browserify -t node-lessify ./src/index.tsx --debug -p [ tsify --noImplicitAny ] > ./build/app.js",
    "start": "budo src/index.tsx:build/app.js -- -t node-lessify -p [ tsify --noImplicitAny ]",
    "clean": "rm -r ./build/*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
